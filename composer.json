{
	"name": "thefox/persons",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.0",
		"twig/twig": "1.30.0",
		"twig/extensions": "1.2.0",
		"laravelcollective/html": "5.0.2",
		"rcrowe/twigbridge": "0.9.4",
		"components/jquery": "2.1.3",
		"doctrine/dbal": "2.5.1"
	},
	"require-dev": {
		"phpunit/phpunit": "4.6.4",
		"doctrine/common": "2.5.0",
		"thefox/utilities": "1.9.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}